<% if (locals.messages) { %>
<ul>
  <% messages.forEach(message => { %>
  <li><%= message %></li>
  <% }) %>
</ul>
<% } %>
<% if (errors) { %>
    <ul class="notice">
      <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
      <% }) %>
    </ul>
    <% } %>
<h1>Your are logged in</h1>    